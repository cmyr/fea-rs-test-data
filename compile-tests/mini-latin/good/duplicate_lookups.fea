# we had a crash when a feature had duplicate lookups, because we were asserting
# that feature lookups were sorted.
# We should probably be deduplicating these lookups, but we are not currently
# (https://github.com/cmyr/fea-rs/issues/92)
languagesystem DFLT dflt;
languagesystem latn dflt;
languagesystem latn HMM;

feature test {

    lookup alt1 {
		sub a by A;
	} alt1;
    lookup alt2 {
        sub b by B;
    } alt2;

    # latn dflt here includes both named lookups
	script latn;
	lookup alt1;
} test;


